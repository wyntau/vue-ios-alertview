{
  "name": "vue-ios-alertview",
  "version": "1.1.1",
  "description": "iOS7+ style alertview service for Vue.",
  "main": "dist/vue-ios-alertview.js",
  "types": "types/index.d.ts",
  "directories": {
    "example": "example"
  },
  "scripts": {
    "clean": "rm -rf dist",
    "build": "npm run clean && rollup -c build/rollup.config.js && uglifyjs -m -o dist/vue-ios-alertview.min.js -- dist/vue-ios-alertview.js",
    "dev": "rollup -c build/rollup.config.js --watch",
    "yarn": "SASS_BINARY_SITE=http://npm.taobao.org/mirrors/node-sass yarn"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Treri/vue-ios-alertview.git"
  },
  "keywords": [
    "Vue",
    "iOS",
    "UIAlertView"
  ],
  "author": "Treri",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Treri/vue-ios-alertview/issues"
  },
  "homepage": "https://github.com/Treri/vue-ios-alertview#readme",
  "devDependencies": {
    "rollup": "^0.41.6",
    "rollup-plugin-buble": "^0.15.0",
    "rollup-plugin-vue": "^2.3.1",
    "rollup-watch": "^4.0.0",
    "uglify-js": "^2.8.22",
    "vue": "^2.2.6"
  }
}
